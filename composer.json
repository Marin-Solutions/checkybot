{
  "name": "checkybot-labs/checkybot",
  "type": "project",
  "description": "Checkybot",
  "keywords": ["laravel", "framework"],
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "ext-dom": "*",
    "bezhansalleh/filament-shield": "^3.2",
    "darkaonline/l5-swagger": "^8.6",
    "filament/filament": "^3.2",
    "guzzlehttp/guzzle": "^7.9",
    "jeffgreco13/filament-breezy": "^2.4",
    "laravel/framework": "^11.9",
    "laravel/horizon": "^5.28",
    "laravel/pulse": "^1.4",
    "laravel/reverb": "^1.6",
    "laravel/sanctum": "^4.0",
    "laravel/telescope": "^5.1",
    "laravel/tinker": "^2.9",
    "laravel/vonage-notification-channel": "^3.3",
    "league/flysystem-aws-s3-v3": "^3.0",
    "league/flysystem-ftp": "^3.0",
    "league/flysystem-sftp-v3": "^3.0",
    "predis/predis": "^2.0",
    "pusher/pusher-php-server": "^7.2",
    "spatie/crawler": "^8.2",
    "spatie/dns": "^2.6",
    "spatie/laravel-flare": "^1.1",
    "spatie/laravel-permission": "^6.9",
    "spatie/ssl-certificate": "^2.6",
    "symfony/http-client": "^7.1",
    "symfony/postmark-mailer": "^7.1",
    "webbingbrasil/filament-copyactions": "^3.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.14",
    "fakerphp/faker": "^1.23",
    "laravel/boost": "^1.0",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
